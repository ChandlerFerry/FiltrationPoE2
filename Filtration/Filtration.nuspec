  <files>
    <file src="*.*" target="lib\net45\" exclude="*.pdb;*.nupkg;*.vshost.*;*.xml;*.log"/>
    <file src="Resources\AlertSounds\*.mp3" target="lib\net45\Resources\AlertSounds\" />
  </files>
